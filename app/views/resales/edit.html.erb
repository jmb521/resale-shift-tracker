<h2>Edit the season and year of the sale</h2>
<form action="/resales/<%= @resale.id%>/edit" method="post" enctype="multipart/form-data">
    <label for="Season">Season: </label>
    <select name="resale[season]" value="<%= @resale.season %>">
        <option></option>
        <option value="fall">Fall</option>
        <option value="spring">Spring</option>
    </select>
    <br />
    <br />
    <label for="year">Year: </label>
    <br />
    <input type="text" name="resale[year]" id="year" value="<%= @resale.year%>"/>
    <br />
    
    <br />
    <input type="submit" name="submit" value="Submit" />
</form>